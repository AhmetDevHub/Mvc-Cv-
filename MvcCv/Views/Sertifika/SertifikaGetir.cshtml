@model MvcCv.Models.Entity.TblSertifikalar
@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>  Sertifika Güncelleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Serifika")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-info">Güncelle</button>
</form>